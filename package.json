{
  "name": "currentjs",
  "packageManager": "yarn@3.6.4",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "res:build": "rescript",
    "res:dev": "rescript build -w",
    "start": "node --experimental-specifier-resolution=node server",
    "production": "NODE_ENV=production yarn start",
    "relay": "relay-compiler",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr src/entry/server.tsx",
    "build": "yarn build:client && yarn build:server",
    "build:vite:server-fix": "perl -i -pe 's/import \\* as ReactRelay/import ReactRelay/' dist/server/assets/*.js",
    "dev:vite:server-fix": "perl -i -pe 's/import \\* as ReactRelay/import ReactRelay/' node_modules/rescript-relay/src/*.mjs",
    "lingui:extract": "lingui extract",
    "lingui:compile": "lingui compile",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "resolutions": {
    "@swc/core": "1.3.56"
  },
  "dependencies": {
    "@linaria/babel-preset": "^4.4.5",
    "@linaria/core": "^4.2.10",
    "@linaria/react": "^4.3.8",
    "@linaria/server": "^4.1.0",
    "@lingui/core": "^4.1.2",
    "@lingui/macro": "^4.1.2",
    "@lingui/react": "^4.1.2",
    "@remix-run/web-fetch": "^4.3.5",
    "@rescript/core": "^0.4.0",
    "@rescript/react": "^0.11.0",
    "@swc/core": "^1.3.60",
    "@vitejs/plugin-react-swc": "^3.3.1",
    "autoprefixer": "^10.4.14",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "express-static-gzip": "^2.1.7",
    "html-react-parser": "^3.0.16",
    "isbot": "^3.6.10",
    "meros": "^1.3.0",
    "postcss": "^8.4.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-relay": "^15.0.0",
    "react-router-dom": "^6.11.2",
    "relay": "^0.8.0-1",
    "relay-runtime": "^15.0.0",
    "rescript": "11.0.0-beta.4",
    "rescript-relay": "0.0.0-version-3-8b552902",
    "serve-static": "^1.15.0",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@linaria/vite": "^4.2.11",
    "@lingui/cli": "^4.1.2",
    "@lingui/conf": "^4.1.2",
    "@lingui/swc-plugin": "4.0.0",
    "@lingui/vite-plugin": "^4.1.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@testing-library/react": "^14.0.0",
    "@types/express": "^4.17.17",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/react-relay": "^14.1.4",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "graphql": "alpha",
    "postcss-flexbugs-fixes": "^5.0.2",
    "relay-compiler": "^15.0.0",
    "rescript-vitest": "^1.2.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^5.0.4",
    "vite": "^4.4.11",
    "vite-plugin-compression2": "^0.9.1",
    "vite-plugin-relay-lite": "^0.4.3",
    "vitest": "^0.31.1"
  }
}
