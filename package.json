{
  "name": "racquetleague",
  "packageManager": "yarn@4.0.2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "res:build": "rescript",
    "res:dev": "rescript build -w",
    "start": "NODE_ENV=development yarn graphql:generate && node --experimental-specifier-resolution=node server",
    "production": "NODE_ENV=production node --experimental-specifier-resolution=node server",
    "graphql:generate": "graphql-codegen",
    "relay": "relay-compiler",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr src/entry/server.tsx",
    "build": "yarn rescript-relay-compiler && yarn build:client && yarn build:server",
    "build:vite:server-fix": "perl -i -pe 's/import \\* as ReactRelay/import ReactRelay/' dist/server/assets/*.js",
    "dev:vite:server-fix": "perl -i -pe 's/import \\* as ReactRelay/import ReactRelay/' node_modules/rescript-relay/src/*.mjs",
    "lingui:extract": "lingui extract",
    "lingui:compile": "lingui compile",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lingui:update-compendium-experimental": "zsh update-compendium.sh",
    "lingui:update-compendium": "msgcat --use-first -o src/locales/jp.po src/locales/jp.po src/locales/jp/**/*.po",
    "lingui:sync-translations": "sh sync-translations.sh"
  },
  "resolutions": {
    "@swc/core": "1.3.106"
  },
  "dependencies": {
    "@glennsl/rescript-json-combinators": "^1.3.0",
    "@linaria/babel-preset": "^4.4.5",
    "@linaria/core": "^6.0.0",
    "@linaria/react": "^6.0.0",
    "@linaria/server": "^6.0.0",
    "@lingui/core": "^4.8.0-next.1",
    "@lingui/macro": "^4.8.0-next.1",
    "@lingui/react": "^4.8.0-next.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@remix-run/web-fetch": "^4.3.5",
    "@rescript/core": "^0.6.0",
    "@rescript/react": "0.12.0-alpha.2",
    "@swc/core": "1.3.106",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "express-static-gzip": "^2.1.7",
    "glob": "^10.3.10",
    "html-react-parser": "^3.0.16",
    "isbot": "^3.6.10",
    "lucide-react": "^0.295.0",
    "meros": "^1.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-relay": "^16.1.0",
    "react-router-dom": "^6.11.2",
    "relay-runtime": "^16.1.0",
    "rescript": "11.0.0-beta.4",
    "rescript-relay": "3.0.0-alpha.4",
    "resolve": "^1.22.8",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@imchhh/storybook-addon-relay": "^0.0.8",
    "@lingui/cli": "^4.8.0-next.1",
    "@lingui/conf": "^4.8.0-next.1",
    "@lingui/swc-plugin": "4.0.0-next.3",
    "@lingui/vite-plugin": "^4.8.0-next.1",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-interactions": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.6",
    "@storybook/react": "^7.4.6",
    "@storybook/react-vite": "^7.4.6",
    "@storybook/testing-library": "^0.2.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@testing-library/react": "^14.0.0",
    "@types/babel-plugin-macros": "^3",
    "@types/express": "^4.17.17",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/react-relay": "^16",
    "@types/relay-test-utils": "^14.1.4",
    "@types/resolve": "^1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@wyw-in-js/vite": "^0.2.2",
    "autoprefixer": "^10.4.16",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-relay": "^16.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-storybook": "^0.6.15",
    "graphql": "alpha",
    "graphql-tag": "^2.12.6",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.32",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^15.1.0",
    "prop-types": "^15.8.1",
    "relay-compiler": "^16.1.0",
    "relay-test-utils": "^16.0.0",
    "rescript-vitest": "^1.2.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "storybook": "^7.4.6",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.4",
    "vite": "^5.0.3",
    "vite-plugin-compression2": "^0.9.1",
    "vite-plugin-relay": "^2.0.0",
    "vitest": "^0.34.6"
  }
}
